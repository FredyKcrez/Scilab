function btnCalcular_MM1N(handles)

    funcprot(0);
    format(7);
    miu = evstr(get(handles.editMiu,'string'));
    lambda = evstr(get(handles.editLambda,'string'));
    rho = lambda/miu;
    t1=evstr(get(handles.edit1, 'string'));
    t2=evstr(get(handles.edit2, 'string'));
    N = evstr(get(handles.editN,'string'));
    n = evstr(get(handles.editClientes,'string'));
    //CALCULO DE Po
    P = 0.0;
    for I=0:N
        P = P + ((factorial(N)./factorial(N-I)).*(rho).^I);
    end
    Po = 1/P;
    //CALCULO DE Pn
    Pn = ((factorial(N)/factorial(N-n))*(rho)^n) * Po;
    //CALCULO DE L
    L = N - (miu/lambda)*(1-Po);
    //CALCULO DE Lq
    Lq=0.0;
    for I=1:N
        Lq  = Lq + (n-1)*Pn;
    end
    //CALCULO DE W
    W = L/lambda;
    //CALCULO DE Wq
    Wq = Lq/lambda;
    //calculo de lambda media
    lambdaMedia = lambda*(N-L);
    //CALCULO DE LA EFICIENCIA
    eficiencia = miu * W;
    set(handles.editRho,'String',string(rho));
    set(handles.ePo,'String',string(Po));
    set(handles.ePn,'String',string(Pn));
    set(handles.eL,'String',string(L));
    set(handles.eLq,'String',string(Lq));
    set(handles.eW,'String',string(W));
    set(handles.eWq,'String',string(Wq));
    set(handles.eLambdaMedia,'String',string(lambdaMedia));
    set(handles.eEficiencia,'String',string(eficiencia));
//    Graficar W y Wq
    Grafica_MMSN(lambda,miu,N,1);

endfunction


