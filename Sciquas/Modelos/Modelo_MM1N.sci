function Grafica_MMSN(lambda,miu,N,s)
    
    c=ones(1,s);
    for i=2:s
      c(i)=c(i-1)*lambda/(miu*(i-1));
    end
   
    ro=(lambda/(miu*s));
    if (ro == 1) //ro igual a 1
       c_mayor_s=c(s)*(N+1);
    else //ro distinto de 1
       c_mayor_s=c(s)*(ro-(ro^(N+2)))/(1-ro);
    end
    vector_p=ones(1,N+s+1);
    vector_p(1)=1/(sum(c)+c_mayor_s);
   
    for i=2:s
       vector_p(i)=c(i)*vector_p(1);
    end
   
    for i=s+1:N+s+1
       vector_p(i)=ro*vector_p(i-1);
    end
    x=linspace(1,3, 100);
    //n2 es un vector 0,1,...k+s-1
    n2=linspace(0,N+s-1,N+s);   
    vector_q=vector_p(n2+1)/(1-vector_p(N+s+1));
    
    variable_a_wqt=ones(1,length(x));
    variable_s_wqt=ones(1,length(x));
    if (N==0)
        variable_b_wqt=0;
    else
        variable_b_wqt=vector_q(s+1)*ones(1,length(x));
    end
   
    for variable_n_wqt=s+1:N+s-1
        variable_a_wqt=variable_a_wqt.*(miu*x*s/(variable_n_wqt-s));
        variable_s_wqt=variable_s_wqt+variable_a_wqt;
        variable_b_wqt=variable_b_wqt+vector_q(variable_n_wqt+1)*variable_s_wqt;
    end
    //Calcula Wq(t)
    wq_t=1-variable_b_wqt.*exp(-miu*x*s);
    
    //Calcular W(t)
    w_t = 1-exp(-(miu - lambda)*x);
    for i=1:length(w_t)
        if (w_t(i)<0)
            w_t(i)=0;
        end
    end
    
    for i=1:length(wq_t)
        if (wq_t(i)<0)
            wq_t(i)=0;
        end
    end
    set(handles.editWq_t,'String',string(wq_t(100)));
    set(handles.editW_t,'String',string(w_t(1)));
    plot(x,w_t,x,wq_t);
    xgrid;
    
endfunction

