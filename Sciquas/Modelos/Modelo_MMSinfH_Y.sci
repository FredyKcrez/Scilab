function btnCalcular_MMSN(handles)

    miu = evstr(get(handles.editMiu,'String'));
    lambda = evstr(get(handles.editLambda,'String'));
    h = evstr(get(handles.editClientes,'string'));
    s = evstr(get(handles.editServ,'String'));
    y = evstr(get(handles.editValorY,'String'));
    
    //Modelo M/M/s/inf/H con Y Repuesto   
    c=ones(1,y+h+1);
    ro=lambda/(miu*s);
    set(handles.editRho,'String',string(ro));
    
    if (s<=y)   //Si s<=y
       for i=2:s+1
         c(i)=c(i-1)*lambda*h/(miu*(i-1));
       end
       for i=s+2:y+1
         c(i)=h*ro*c(i-1);
       end
       for i=y+2:y+h+1
         c(i)=c(i-1)*ro*(h+y-(i-1)+1);
       end
   else				//s>y
       for i=2:y+1
         c(i)=c(i-1)*lambda*h/(miu*(i-1));
       end
       for i=y+2:s+1
         c(i)=(h+y-(i-1)+1)*lambda/((i-1)*miu)*c(i-1);
       end
       for i=s+2:y+h+1
         c(i)=(h+y-(i-1)+1)*ro*c(i-1);
       end
   end
   
   vector_p=ones(1,y+h+1);
   
   vector_p(1)=1/sum(c);
   
   for i=2:y+h+1
      vector_p(i)=c(i)*vector_p(1);
   end
   
   //n es un vector formado por 1,2,...y+h
   n=linspace(1,y+h,y+h);
   
   L=sum(n.*vector_p(2:y+h+1));
   set(handles.eL,'String',string(L));
            
   lambdabarra=0;
   for i=y:y+h
        lambdabarra=(i-y)*vector_p(i+1)+lambdabarra;
   end
   
   lambdabarra=lambda*(h-lambdabarra);
   set(handles.eLambdaMedia,'String',string(lambdabarra));
   
   w_n=L/lambdabarra;
   set(handles.eW,'String',string(w_n));
   
   wq_n=w_n-1/miu;
   set(handles.eWq,'String',string(wq_n));
   
   lq_n=lambdabarra*wq_n;
   set(handles.eLq,'String',string(lq_n));
   
   ro_barra=lambdabarra/(s*miu);
   set(handles.editRhoBarra,'String',string(ro_barra));
   
   Grafica_MMSinfH_Y(lambda,miu,h,s);

endfunction
function Grafica_MMSinfH_Y(lambda,miu,h,s,y)
        x=linspace(0,2, 100);
        aux=0;
        for i=y:y+h
            aux=(i-y)*vector_p(i+1)+aux;
        end
        aux=h-aux;
   
        for i=1:y
            vector_q(i)=h*vector_p(i)/aux;
        end
        for i=y+1:y+h
            vector_q(i)=(h+y-(i-1))*vector_p(i)/aux;
        end
        
        variable_a_wqt=ones(1,length(x));
        variable_s_wqt=ones(1,length(x));
        variable_b_wqt=vector_q(s+1)*ones(1,length(x));
        
        for variable_n_wqt=s+1:h-1
            variable_a_wqt=variable_a_wqt.*(miu*x*s/(variable_n_wqt-s));
            variable_s_wqt=variable_s_wqt+variable_a_wqt;
            variable_b_wqt=variable_b_wqt+vector_q(variable_n_wqt+1)*variable_s_wqt;
        end
   
        //Calcula Wq(t)
        wq_t=1-variable_b_wqt.*exp(-miu*x*s);
        
        w_t=1-exp(-(miu-lambda)*x);
        for i=1:length(w_t)
          if (w_t(i)<0)
            w_t(i)=0;
          end
        end
    
        for i=1:length(wq_t)
          if (wq_t(i)<0)
            wq_t(i)=0;
          end
        end
   
   plot(x,w_t,'r-',x,wq_t,'b');
   xgrid;
  
endfunction



